{
  "compilerOptions": {
    "lib": [
      "es2017",
    ],
    "outDir": "dist",
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "experimentalDecorators": true,
    "strictNullChecks": true,
    "emitDecoratorMetadata": true,
    "esModuleInterop": true,
    "target": "es2020",
    "module": "ESNext",
  },
  "exclude": []
}